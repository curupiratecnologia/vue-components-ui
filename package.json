{
  "name": "@curupira/vue-components-ui",
  "version": "0.0.1",
  "private": false,
  "description": "Vue Components used in our project of Curupira Tecnologia",
  "author": {
    "name": "Leandro Celes",
    "email": "leandroceles@gmail.com",
    "url": "https://curupiratecnologia.com.br"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-lib": "npm run build-lib:library",
    "build-lib:library": "rm -rf dist/* && vue-cli-service build --target lib --name curupira-ui src/library-entry.js --dest dist/",
    "serve:lib": "vue-cli-service build --target lib --name vue-mapbox src/library-entry.js --dest dist/ --watch",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service  styleguidist:build --verbose"
  },
  "main": "dist/curupira-ui.umd.js",
  "module": "dist/curupira-ui.umd.js",
  "dependencies": {
    "@curupira/grid-system": "^0.1.1",
    "@deck.gl/core": "^8.2.3",
    "@deck.gl/layers": "^8.2.3",
    "@deck.gl/mapbox": "^8.2.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "core-js": "^3.6.4",
    "mapbox-gl": "^1.10.0",
    "vue": "^2.6.11",
    "vue-static": "0.0.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": ">6.0.0",
    "@storybook/addon-docs": ">6.0.0",
    "@storybook/addon-knobs": ">6.0.0",
    "@storybook/addon-links": ">6.0.0",
    "@storybook/storybook-deployer": "^2.8.6",
    "@storybook/vue": ">6.0.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-storybook": "~1.3.0",
    "vue-cli-plugin-styleguidist": "~4.19.0",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {},
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "homepage": "https://curupiratecnologia.github.io/grid-system/",
  "keywords": [
    "ui",
    "vue",
    "components",
    "dropdown",
    "data-loader",
    "interface"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/curupiratecnologia/vue-components-ui.git"
  },
  "sideEffects": false
}
